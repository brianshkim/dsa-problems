const subsets = (elements) => {
  // todo
  if (elements.length===0) return [[]]
 let first = elements[0]
 console.log(first)
 let arr = []
 let inners = subsets(elements.slice(1))
  for(inner of inners){
    arr.push([first, ...inner]  )
    
  }
  console.log(first, arr)
  
  return [...arr, ...subsets(elements.slice(1))]
  
};
