var distanceTraveled = function(mainTank, additionalTank) {
    let count = 0
    while(mainTank > 0){
        mainTank -= 1
        count += 1
        if (count%5 === 0 && additionalTank >= 1){
            mainTank += 1
            additionalTank -= 1
        }
       
        
    }
    return count*10
};
